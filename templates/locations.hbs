{{#*inline "page"}}
    <table>
        <tr><th>ID</th><th>Latitude</th><th>Longitude</th></tr>
        {{#each .}}
            <tr>
                <td>{{id}}</td>
                <td>{{lat}}</td>
                <td>{{lon}}</td>
            </tr>
        {{/each}}
    </table>
{{/inline}}
{{> layout}}